{
  "name": "@cirjson/core",
  "version": "1.0.0",
  "scripts": {
    "build": "unbuild"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./parse": {
      "import": "./dist/parse.mjs",
      "types": "./dist/parse.d.ts",
      "require": "./dist/parse.cjs"
    },
    "./stringify": {
      "import": "./dist/stringify.mjs",
      "types": "./dist/stringify.d.ts",
      "require": "./dist/stringify.cjs"
    },
    "./globals": {
      "import": "./dist/globals.mjs",
      "types": "./dist/globals.d.ts",
      "require": "./dist/globals.cjs"
    }
  },
  "files": ["dist", "src"],
  "dependencies": {},
  "devDependencies": {
    "unbuild": "^2.0.0"
  }
}
